{
  "taskRoleArn": "arn:aws:iam::817083319299:role/shifamed-task-role",
  "containerDefinitions": [
    {
      "name": "<container_name>",
      "image": "<container_image>",
      "memoryReservation": 128,
      "environment": [
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-west-2",
          "awslogs-stream-prefix": "doc-fb",
          "awslogs-group": "ecs-dev"
        }
      },
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 3000,
          "hostPort": 0
        }
      ],
      "dockerLabels": {},
      "essential": true,
      "volumesFrom": []
    }
  ]
}